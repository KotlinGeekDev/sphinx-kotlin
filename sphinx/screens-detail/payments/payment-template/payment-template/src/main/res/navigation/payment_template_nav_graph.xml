<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/payment_template_nav_graph"
    app:startDestination="@+id/navigation_payment_template_fragment">

    <fragment
        android:id="@+id/navigation_payment_template_fragment"
        android:name="chat.sphinx.payment_template.ui.PaymentTemplateFragment"
        tools:layout="@layout/fragment_payment_template">
        <argument
            android:name="argContactId"
            app:argType="long" />
        <argument
            android:name="argChatId"
            app:argType="long" />
        <argument
            android:name="argAmount"
            app:argType="long" />
        <argument
            android:name="argMessage"
            app:argType="string" />
    </fragment>

</navigation>
